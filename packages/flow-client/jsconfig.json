{
  "compilerOptions": {
    "jsx": "react",
    "checkJs": false,
    "allowJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "module": "ESNext",
    "moduleResolution": "bundler"
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": ["src/**/*.js", "src/**/*.jsx"],
  "exclude": ["node_modules", "build", "**/*.ts", "**/*.tsx"]
}
